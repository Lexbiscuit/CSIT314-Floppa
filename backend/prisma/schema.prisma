// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User_Account {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
}

model Cafe_Staff {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
}

model Cafe_Owner {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
} 

model Cafe_Manager {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
}

model System_Administrator {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
}
